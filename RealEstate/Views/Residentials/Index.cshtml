@model IEnumerable<RealEstate.Models.Residential>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Property.Category)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Built_Up_Length)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Built_Up_Width)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Land_Length)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Land_Width)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NoOfBedrooms)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Property.Category)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Built_Up_Length)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Built_Up_Width)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Land_Length)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Land_Width)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Location)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NoOfBedrooms)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
